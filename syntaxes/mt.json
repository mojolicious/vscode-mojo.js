{
  "name": "Embedded JavaScript (mojo.js)",
  "scopeName": "text.html.mt",
  "injectionSelector": "L:text.html",
  "patterns": [
    { "include": "#comment-block" },
    { "include": "#code-block" },
    { "include": "#code-line" },
    { "include": "#code-line-indented" }
  ],
  "repository": {
    "comment-block": {
      "name": "comment.block.mt",
      "begin": "<%#",
      "beginCaptures": {
        "0": {
          "name": "punctuation.definition.comment.begin.mt"
        }
      },
      "end": "%>",
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.comment.end.mt"
        }
      }
    },
    "code-block": {
      "name": "source.js.mt.block",
      "begin": "(<%={,2})",
      "beginCaptures": {
        "0": {
          "name": "support.begin.mt"
        }
      },
      "end": "(=?%>)",
      "endCaptures": {
        "0": {
          "name": "support.end.mt"
        }
      },
      "patterns": [
        {
          "include": "source.js"
        }
      ]
    },
    "code-line": {
      "begin": "(?<=\\s{2})(%={,2})",
      "beginCaptures": {
        "0": {
          "name": "support.begin.mt"
        }
      },
      "end": "$",
      "name": "source.js.mt.indented",
      "patterns": [
        {
          "include": "source.js"
        }
      ]
    },
    "code-line-indented": {
      "name": "source.js.mt.line",
      "begin": "^(%={,2})",
      "beginCaptures": {
        "0": {
          "name": "support.begin.mt"
        }
      },
      "end": "$",
      "patterns": [
        {
          "include": "source.js"
        }
      ]
    }
  }
}
