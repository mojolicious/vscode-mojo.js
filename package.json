{
  "name": "javascript-mt-support",
  "displayName": "mojo.js",
  "description": "VS Code support for mojo.js",
  "version": "2.1.2",
  "publisher": "kraih",
  "repository": {
    "type": "git",
    "url": "https://github.com/mojolicious/vscode-mojo.js"
  },
  "engines": {
    "vscode": "^1.5.0"
  },
  "license": "MIT",
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "mojo",
    "javascript",
    "template"
  ],
  "icon": "logo.png",
  "galleryBanner": {
    "color": "#ffffff",
    "theme": "light"
  },
  "contributes": {
    "grammars": [
      {
        "language": "mt",
        "scopeName": "text.html.mt",
        "path": "./syntaxes/mt.json",
        "embeddedLanguages": {
          "source.css": "css",
          "source.js": "javascript"
        }
      },
      {
        "injectTo": [
          "source.js",
          "source.js.jsx",
          "source.jsx",
          "source.ts",
          "source.tsx"
        ],
        "scopeName": "mt.literal",
        "path": "./syntaxes/mt-literal.json"
      }
    ],
    "languages": [
      {
        "id": "mt",
        "aliases": [
          "mt",
          "Embedded JavaScript"
        ],
        "extensions": [
          ".mt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "snippets": [
      {
        "language": "html",
        "path": "./snippets/snippets.json"
      }
    ]
  }
}
