{
  "name": "SQL Template Literal (mojo.js)",
  "fileTypes": ["js", "jsx", "ts", "tsx", "vue", "mjs", "cjs", "html", "tmpl"],
  "injectionSelector": "L:source -comment -string",
  "patterns": [
    {
      "name": "string.template.ts",
      "begin": "\\b(sql)\\s*(?=`)",
      "beginCaptures": {
        "1": {
          "name": "entity.name.function.tagged-template.ts"
        }
      },
      "end": "(?<=`)",
      "patterns": [
        {
          "include": "#embedded-sql"
        }
      ]
    }
  ],
  "scopeName": "sql.literal",
  "repository": {
    "embedded-sql": {
      "name": "string.template.ts",
      "contentName": "meta.embedded.block.tmpl",
      "begin": "`",
      "beginCaptures": {
        "0": { "name": "punctuation.definition.string.template.begin.js" }
      },
      "end": "`",
      "endCaptures": {
        "0": { "name": "punctuation.definition.string.template.end.js" }
      },
      "patterns": [
        { "include": "source.ts#template-substitution-element" },
        { "include": "source.ts#string-character-escape" },
        { "include": "source.sql" },
        { "include": "source.plpgsql.postgres" },
        { "match": "." }
      ]
    }
  }
}
